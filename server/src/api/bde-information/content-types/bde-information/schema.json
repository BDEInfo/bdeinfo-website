{
  "kind": "singleType",
  "collectionName": "bde_informations",
  "info": {
    "singularName": "bde-information",
    "pluralName": "bde-informations",
    "displayName": "BDE Informations",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": false,
      "default": "contact@bdeinfo.org"
    },
    "phone": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "default": "45.785981, 4.883337",
      "required": false
    },
    "statuts": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "mandat_actuel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mandat.mandat"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "address": {
      "type": "string"
    },
    "description_short": {
      "type": "richtext",
      "required": true
    },
    "cards": {
      "type": "component",
      "component": "custom-types.description-card",
      "repeatable": true
    }
  }
}
